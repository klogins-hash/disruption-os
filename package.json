{
  "name": "disruption-os-versioning",
  "version": "1.0.0",
  "description": "WORM-based versioning system for Disruption OS with 11ty static site generation",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"mintlify dev\"",
    "dev:api": "nodemon server.js",
    "dev:11ty": "eleventy --serve",
    "build": "mintlify install",
    "start": "node server.js",
    "db:migrate": "node scripts/migrate.js",
    "version:create": "node scripts/create-version.js"
  },
  "keywords": [
    "versioning",
    "worm",
    "11ty",
    "disruption-os"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "marked": "^11.1.1",
    "simple-git": "^3.22.0",
    "dayjs": "^1.11.10"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}
